        SpamLoop = RunService.Heartbeat:Connect(function()
            SendMessage:FireServer(text)
        end)

    elseif cmd == ".unspam" then
        if SpamLoop then
            SpamLoop:Disconnect()
            SpamLoop = nil
        end

    elseif cmd == ".say" and text ~= "" then
        SendMessage:FireServer(text)

    elseif cmd == ".freeze" then
        IsFrozen = true
        local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
