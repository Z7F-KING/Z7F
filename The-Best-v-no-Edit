-- Ù…ØªØºÙŠØ± Ù„ØªØªØ¨Ø¹ Ø§Ù„Ø²Ø±/Ø§Ù„Ø³ÙƒØ±Ø¨Øª Ø§Ù„Ù†Ø´Ø·
local activeScript = nil

-- Ø¯Ø§Ù„Ø© Ù„ØªØ´ØºÙŠÙ„ Ø§Ù„Ø³ÙƒØ±Ø¨Øª Ù…Ø¹ Glow Ù…Ø¤Ù‚Øª Ø«Ø§Ù†ÙŠØªÙŠÙ† ÙˆØ¥ÙŠÙ‚Ø§Ù Ø§Ù„Ø³Ø§Ø¨Ù‚
local function runScript(button, url)
    -- Ø¥ÙŠÙ‚Ø§Ù Glow Ø§Ù„Ø³Ø§Ø¨Ù‚ Ø¥Ø°Ø§ ÙƒØ§Ù† Ø²Ø± Ø¢Ø®Ø± Ù†Ø´Ø·
    if activeScript and activeScript ~= button then
        if activeScript.Glow then
            activeScript.Glow:Destroy()
            activeScript.Glow = nil
        end
    end

    -- ØªØ´ØºÙŠÙ„ Ø§Ù„Ø³ÙƒØ±Ø¨Øª
    loadstring(game:HttpGet(url))()

    -- Glow Ù…Ø¤Ù‚Øª 2 Ø«Ø§Ù†ÙŠØ©
    local gradient = Instance.new("UIGradient", button)
    gradient.Rotation = 0
    gradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(200,200,200)),
        ColorSequenceKeypoint.new(0.5, Color3.fromRGB(150,150,150)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(200,200,200))
    })
    gradient.Offset = Vector2.new(-1,0)
    local tween = TweenService:Create(gradient, TweenInfo.new(1.2, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1, true), {Offset = Vector2.new(1,0)})
    tween:Play()
    button.Glow = gradient

    task.delay(2, function()
        tween:Cancel()
        gradient:Destroy()
        button.Glow = nil
    end)

    -- ØªØ¹ÙŠÙŠÙ† Ø§Ù„Ø²Ø± Ø§Ù„Ù†Ø´Ø·
    activeScript = button
end

