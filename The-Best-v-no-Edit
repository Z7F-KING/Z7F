local function startSpin()
    while spinActive do
        task.wait()
        pcall(function()
            LocalPlayer.Character.HumanoidRootPart.CFrame *= CFrame.Angles(0, math.rad(55), 0)
        end)
    end
end

SpinButton.MouseButton1Click:Connect(function()
    spinActive = not spinActive

    if spinActive then
        SpinButton.BackgroundColor3 = Color3.fromRGB(255,255,255)
        spinGlow, spinTween = createGlow(SpinButton)

        LocalPlayer.CharacterAdded:Connect(function()
            task.wait(1)
            if spinActive then
                spinThread = task.spawn(startSpin)
            end
        end)

        spinThread = task.spawn(startSpin)
    else
        SpinButton.BackgroundColor3 = Color3.fromRGB(25,25,25)
        if spinGlow then spinGlow:Destroy() end
        spinGlow = nil
        spinTween = nil
        spinThread = nil
    end
end)

