local otherActive = false
local otherGlow = nil
local otherLoop = nil

-- Ø¯Ø§Ù„Ø© Ø¨Ø¯Ø¡ Ø§Ù„Ø³Ø¨Ø§Ù…
local function startOtherAction()
    otherLoop = task.spawn(function()
        while otherActive do
            local text = OtherTextBox.Text
            if #text >= 1 then
                local args = { text }
                pcall(function()
                    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("SendMessage"):FireServer(unpack(args))
                end)
            end
            task.wait(0.3) -- Ø³Ø±Ø¹Ø© Ø³Ø¨Ø§Ù… 0.3 Ø«Ø§Ù†ÙŠØ©
        end
    end)
end

-- Ø¯Ø§Ù„Ø© Ø¥ÙŠÙ‚Ø§Ù Ø§Ù„Ø³Ø¨Ø§Ù…
local function stopOtherAction()
    otherActive = false
    if otherGlow then 
        otherGlow:Destroy()
        otherGlow = nil
    end
    if otherLoop then
        otherLoop = nil
    end
    OtherActionButton.BackgroundColor3 = Color3.fromRGB(25,25,25)
end

-- Ø§Ù„Ø¶ØºØ· Ø¹Ù„Ù‰ Ø§Ù„Ø²Ø±
OtherActionButton.MouseButton1Click:Connect(function()
    otherActive = not otherActive
    if otherActive then
        OtherActionButton.BackgroundColor3 = Color3.fromRGB(255,255,255)
        otherGlow,_ = createGlowText(OtherActionButton) -- Ø§Ø³ØªØ®Ø¯Ù… Ø¯Ø§Ù„Ø© Glow Ø§Ù„Ù…ÙˆØ¬ÙˆØ¯Ø© Ù…Ø³Ø¨Ù‚Ø§Ù‹
        startOtherAction()
    else
        stopOtherAction()
    end
end)

