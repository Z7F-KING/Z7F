        startHeadSit(targetPlayer)
    else
        HeadSitButton.BackgroundColor3 = Color3.fromRGB(25,25,25)
        if headSitGlow then headSitGlow:Destroy() headSitGlow = nil end
        stopHeadSit()
    end
end)

