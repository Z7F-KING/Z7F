-- Tween Ù„ØªØ­Ø±ÙŠÙƒ Ø§Ù„Ù„Ù…Ø¹Ø©
local function animateGlow()
    local tweenInfo = TweenInfo.new(1.2, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1, true)
    local tween = TweenService:Create(glowGradient, tweenInfo, {Offset = Vector2.new(1,0)})
    glowGradient.Offset = Vector2.new(-1,0)
    tween:Play()
end

animateGlow()

