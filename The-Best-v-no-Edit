    if autoPrepActive then
        AutoPrepButton.BackgroundColor3 = Color3.fromRGB(255,255,255)
        autoPrepGlow,_ = createGlowText(AutoPrepButton)

        autoPrepLoop = task.spawn(function()
            while autoPrepActive do
                pcall(runAutoPrep)
                task.wait(6)
            end
        end)
    else
