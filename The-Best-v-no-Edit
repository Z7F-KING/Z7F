	noclipActive = not noclipActive

	if noclipActive then
		NoclipButton.BackgroundColor3 = Color3.fromRGB(255,255,255)
		noclipGlow,_ = createButtonGlow(NoclipButton)
		enableNoclip()

	else
		NoclipButton.BackgroundColor3 = Color3.fromRGB(25,25,25)
		if noclipGlow then noclipGlow:Destroy() noclipGlow = nil end
		disableNoclip()
	end
end)

local TweenService = game:GetService("TweenService")
local Players = game:GetService("Players")
