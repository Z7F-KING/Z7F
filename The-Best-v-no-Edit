local function createGlowText(button)
    local gradient = Instance.new("UIGradient", button)
    gradient.Rotation = 0
    gradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(200,200,200)),
        ColorSequenceKeypoint.new(0.5, Color3.fromRGB(150,150,150)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(200,200,200))
    })
    gradient.Offset = Vector2.new(-1,0)
    local tween = TweenService:Create(gradient, TweenInfo.new(1.2, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1, true), {Offset = Vector2.new(1,0)})
    tween:Play()
    return gradient,tween
end

local activePage = "Ø§Ø³ØªÙ‡Ø¯Ø§Ù"
local pageGlowLeft, pageGlowRight = nil, nil
local function activatePage(page)
    activePage = page
    PageLeftButton.BackgroundColor3 = Color3.fromRGB(25,25,25)
    PageRightButton.BackgroundColor3 = Color3.fromRGB(25,25,25)
    PageLeftButton.TextColor3 = Color3.fromRGB(255,255,255)
    PageRightButton.TextColor3 = Color3.fromRGB(255,255,255)
    if pageGlowLeft then pageGlowLeft:Destroy() pageGlowLeft=nil end
    if pageGlowRight then pageGlowRight:Destroy() pageGlowRight=nil end

    if page=="Ø§Ø³ØªÙ‡Ø¯Ø§Ù" then
        PageLeftButton.BackgroundColor3 = Color3.fromRGB(0,0,0)
        pageGlowLeft,_ = createGlowText(PageLeftButton)
        Page_Target.Visible = true
        Page_Other.Visible = false
    else
        PageRightButton.BackgroundColor3 = Color3.fromRGB(0,0,0)
        pageGlowRight,_ = createGlowText(PageRightButton)
        Page_Target.Visible = false
        Page_Other.Visible = true
    end
end

PageLeftButton.MouseButton1Click:Connect(function() activatePage("Ø§Ø³ØªÙ‡Ø¯Ø§Ù") end)
PageRightButton.MouseButton1Click:Connect(function() activatePage("Ø§Ø®Ø±Ù‰") end)
activatePage(activePage)

-- Ø§Ù„Ø¨Ø­Ø« Ø§Ù„ÙÙˆØ±ÙŠ Ù…Ù† 3 Ø£Ø­Ø±Ù ÙÙ…Ø§ ÙÙˆÙ‚
NickTextBox:GetPropertyChangedSignal("Text"):Connect(function()
    local search = NickTextBox.Text:lower()
    if #search < 3 then
        NickLabel.Text = "NickName"
        UserLabel.Text = "User"
        MidImage.Image = "rbxassetid://7992557358"
        targetedPlayerId = nil
        return
    end
    for _, plr in pairs(Players:GetPlayers()) do
        local name = plr.Name:lower()
        local nick = plr.DisplayName:lower()
        if name:sub(1, #search) == search or nick:sub(1, #search) == search then
            MidImage.Image = Players:GetUserThumbnailAsync(plr.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
            NickLabel.Text = plr.DisplayName
            UserLabel.Text = plr.Name
            showTargetNotification(plr)
            break
        end
    end
end)

-- Ø§Ù„Ø³Ø­Ø¨
local dragging, dragStart, startPos = false, nil, nil
local function update(input)
    local delta = input.Position - dragStart
    Frame.Position = UDim2.new(startPos.X.Scale,startPos.X.Offset+delta.X,startPos.Y.Scale,startPos.Y.Offset+delta.Y)
    updateTopImagePosition()
end
Frame.InputBegan:Connect(function(input)
    if input.UserInputType==Enum.UserInputType.MouseButton1 or input.UserInputType==Enum.UserInputType.Touch then
        dragging=true
        dragStart=input.Position
        startPos=Frame.Position
        input.Changed:Connect(function()
            if input.UserInputState==Enum.UserInputState.End then dragging=false end
        end)
    end
end)
UIS.InputChanged:Connect(function(input)
    if dragging and (input.UserInputType==Enum.UserInputType.MouseMovement or input.UserInputType==Enum.UserInputType.Touch) then
        update(input)
    end
end)

