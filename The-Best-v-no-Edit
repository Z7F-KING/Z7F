-- Ø­Ù…Ø§ÙŠØ© Ø§Ù„Ø§Ø³ØªÙ‡Ø¯Ø§Ù Ø§Ù„Ø®Ø§Ø±Ø¬ÙŠ
-----------------------------------------------------
_G.IsProtected = function(target)
	if not target then return false end
	target = target:lower()

	local blocked = false

	for uname,_ in pairs(ProtectedUsers) do
		if IsProtectedUserInServer(uname) then
			-- Ù…Ù†Ø¹ Ù…Ù† Ø£ÙˆÙ„ Ø«Ù„Ø§Ø« Ø£Ø­Ø±Ù ÙÙ‚Ø·
			if #target >= 3 then
				if uname:lower():sub(1,3) == target:sub(1,3) then
					ShowProtectedMessage(uname)
					blocked = true
				end

				local pl = Players:FindFirstChild(uname)
				if pl and pl.DisplayName:lower():sub(1,3) == target:sub(1,3) then
					ShowProtectedMessage(uname)
					blocked = true
				end
			end
		end
	end

	return blocked
end

_G.ProtectToolClick = function(target)
	return _G.IsProtected(target)
end

